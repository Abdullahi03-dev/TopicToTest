datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}




generator client{
  provider="prisma-client-js"
  output  ="../src/generated"
}


model User {
  id       String   @id @default(uuid())
  name     String
  email    String   
  password String
  createdAt DateTime @default(now())
  

  @@unique([email])
  @@map("User")
}
